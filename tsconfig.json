{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom", "es2017", "es2019", "es2020"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    // "baseUrl": "src/",
    "baseUrl": "./src/",
    "paths": {
      "@app/*":["*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@pageBackend/*": ["page/backend/*"],
      "@pageFrontend/*": ["page/frontend/*"],
      "@provider/*": ["provider/*"],
      "@api/*": ["data/*"],
      "@typeTS/*": ["typeTS/*"]
    },
    "noEmitOnError": true,
    "noImplicitAny": false,
    "pretty": true
  },
  "exclude": ["node_modules"],
  "include": [
    "src"
  ]
}